<section class="Candidato-conteiner">
    @for(int i = 0; i<ViewBag.ListaCandidatos.Count; i++){
        <div>
            <h2>@ViewBag.ListaCandidatos[i].Nombre @ViewBag.ListaCandidatos[i].Apellido</h2>
            <img src="~/Imagenes/@ViewBag.ListaCandidatos[i].Foto">
            <a href='@Url.Action("VerDetalleCandidato", new{idPartido=ViewBag.Partido.IdPartido})'>Ver detalle de candidato</a>
            <a href='@Url.Action("EliminarCandidato", new{idPartido=ViewBag.Partido.IdPartido, idCandidato=ViewBag.ListaCandidatos[i].IdCandidato})'>Eliminar candidato</a>
        </div>
    }
</section>
<section class="link-central">
    <a href='@Url.Action("AgregarCandidato", new{idPartido=ViewBag.Partido.IdPartido})'>Agregar candidato</a>
</section>