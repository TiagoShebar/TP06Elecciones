<form action='@Url.Action("GuardarCandidato")' method="post">
    <input type="hidden" name="IdPartido" value="@ViewBag.IdPartido">
    <input type="text" name="Apellido" placeholder="Apellido">
    <input type="text" name="Nombre" placeholder="Nombre">
    <input type="date" name="FechaNacimiento" placeholder="Fecha de nacimiento">
    <input type="text" name="Foto" placeholder="URL de la foto">
    <input type="text" name="Postulacion" placeholder="Puesto del partido">
    <input type="submit" value="Enviar Datos">
</form>
